{
    "name": "ts-package",
    "version": "1.0.0",
    "description": "Easily create Typescript node.js packages from this boilerplate",
    "main": "lib/",
    "types": "lib/",
    "author": "Josh Merlino",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/JoshMerlino/ts-package.git"
    },
    "engines": {
        "node": "16x"
    },
    "scripts": {
        "watch-ts": "tsc -w",
        "watch-js": "nodemon .",
        "dev": "concurrently \"npm:watch-*\"",
        "start": "node .",
        "build": "tsc"
    },
    "devDependencies": {
        "@types/mkdirp": "^2.0.0",
        "@types/node": "^20.14.1",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "nodemon": "^3.1.3",
        "typescript": "^5.4.5",
        "typescript-eslint": "7.11.0"
    },
    "dependencies": {
        "async-require-context": "^1.8.15",
        "chalk": "^5.0.1",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "mkdirp": "^3.0.1"
    }
}

{
    "name": "ts-package",
    "version": "1.0.0",
    "description": "Easily create Typescript node.js packages from this boilerplate",
    "main": "lib/",
    "types": "lib/",
    "author": "Josh Merlino",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/JoshMerlino/ts-package.git"
    },
    "engines": {
        "node": "16x"
    },
    "scripts": {
        "watch-ts": "tsc -w",
        "watch-js": "nodemon .",
        "dev": "concurrently \"npm:watch-*\"",
        "start": "node .",
        "build": "tsc"
    },
    "devDependencies": {
        "@types/mkdirp": "^1.0.2",
        "@types/node": "^18.8.5",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "concurrently": "^7.4.0",
        "eslint": "^8.25.0",
        "nodemon": "^2.0.20",
        "typescript": "^4.8.4",
        "typescript-eslint": "0.0.1-alpha.0"
    },
    "dependencies": {
        "async-require-context": "^1.8.15",
        "chalk": "^5.0.1",
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.3",
        "mkdirp": "^1.0.4"
    }
}
